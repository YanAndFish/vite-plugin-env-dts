{
  "name": "@yafh/vite-plugin-env-dts",
  "version": "0.1.2",
  "description": "Auto generate env file declare for typescript of Vite plugins",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "test": "vite -c test/vite.config.ts"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "tsup": "^6.5.0",
    "typescript": "^4.7.3",
    "vite": "^2.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YanAndFish/vite-plugin-env-dts.git"
  },
  "keywords": [
    "vite",
    "env",
    "typescript"
  ],
  "author": "YanAndFish <383904425@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YanAndFish/vite-plugin-env-dts/issues"
  },
  "homepage": "https://github.com/YanAndFish/vite-plugin-env-dts#readme",
  "packageManager": "pnpm@7.4.0",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "fast-glob": "^3.2.11"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist": "./dist",
    "./package.json": "./package.json"
  }
}